#include <stdio.h>
#include <string.h>
int main()
{
    char a[1000];  
    int i,n,c=0;
 
    printf("Enter  the word : ");
    gets(a);
    n=strlen(a);
 for(i=0;i<n/2;i++)  
{
    	if(a[i]==a[n-i-1])
    	c++;
}
if(c==i)
 	    printf("It is a palindrome");
else
        printf("It is not a palindrome");
 	 return(0);
}
